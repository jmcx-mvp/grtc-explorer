ps: [\"productData\", \"productIndex\"],\n  data() {\n    return {\n      querying: false,\n      poolInfo: null,\n    }\n  },\n  mounted() {\n    this.loopGetInfo()\n    setInterval(async _=> {\n      await this.loopGetInfo()\n    }, 5000)\n  },\n\n  methods: {\n    select() {\n      this.$router.push({ name: \"actions\", params: { tx_id: this.productData.tx, pool_info: this.poolInfo, pool_idx: this.productIndex } });\n    },\n\n    async loopGetInfo() {\n      if(this.querying) {\n        return\n      }\n\n      this.querying =  true\n      this.poolInfo = await opt.poolInfo(this.productIndex)\n      window.allPoolInfo[this.productIndex] = this.poolInfo\n      this.querying = false\n    }\n  }\n}\n</script>\n\n<style rel=\"stylesheet/scss\" lang=\"scss\" scoped>\n@import \"../../styles/colors.scss\";\n@import \"../../styles/fonts.scss\";\n\n.product-div {\n  border-width: 1px;\n  border-style: solid;\n  border-color: $divBorderColor;\n  border-image: initial;\n  border-radius: 5px;\n  padding: 25px 15px 15px ;\n  margin-top: 15px !important;\n\n  .product-logo {\n    width: 48px;\n    height: 48px;\n  }\n  .product-logo-div {\n    background-color: $bg2;\n    font-size: 36px;\n    height: 80px;\n    width: 80px;\n    border-radius: 40px;\n    -webkit-box-align: center;\n    align-items: center;\n    display: flex;\n    -webkit-box-pack: center;\n    justify-content: center;\n    box-shadow: $btnBoxShadow-sushi 4px 4px 8px inset, $btnBoxShadow-sushi-2 -6px -6px 12px inset;\n    margin: 0 auto 16px;\n  }\n\n  .pdt-name {\n    font-family: $fontTit;\n    font-size: 24px;\n    font-weight: 700;\n  }\n\n  .pdt-txt {\n    font-family: $fontTxt;\n    font-size: 16px;\n    font-weight: 400;\n  }\n\n  .btn-select {\n    -webkit-box-align: center;\n    align-items:center;\n    background-color: $btnBg2;\n    border: 0;\n    border-radius: 5px;\n    box-shadow: $btnBoxShadow-sushi 2px 2px 4px, $btnBoxShadow-sushi-2 -2px -2px 4px -1px;\n    color: $bg2;\n    cursor: pointer;\n    display: flex;\n    font-size: 16px;\n    font-weight: 700;\n    height: 56px;\n    -webkit-box-pack: center;\n    justify-content: center;\n    outline: none;\n    padding-left: 24px;\n    padding-right: 24px;\n    width: 100%;\n    margin: 20px 0 !important;\n  }\n  .btn-select:hover,\n  .btn-select:focus,\n  .btn-select:active {\n    background-color: $divBorderColor;\n    color: $bg2;\n    border: 0;\n    outline: none;\n  }\n\n  .pdt-apy {\n    background-color: $btnFontColor;\n    color: $txtColor;\n    border-width: 1px;\n    border-style: solid;\n    border-color: $divBorderColor;\n    border-image: initial;\n    border-radius: 3px;\n    font-size: 12px;\n    padding: 5px;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/dashboard"}]}